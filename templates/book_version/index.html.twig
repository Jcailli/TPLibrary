{% extends 'base.html.twig' %}

{% block title %}BookVersion index{% endblock %}

{% block body %}
    <h1>BookVersion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for book_version in book_versions %}
            <tr>
                <td>{{ book_version.id }}</td>
                <td>{{ book_version.name }}</td>
                <td>
                    <a href="{{ path('app_book_version_show', {'id': book_version.id}) }}">show</a>
                    <a href="{{ path('app_book_version_edit', {'id': book_version.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_book_version_new') }}">Create new</a>
{% endblock %}
